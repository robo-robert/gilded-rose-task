{
  "name": "gilded-rose-task",
  "version": "0.0.0",
  "main": "src/index.ts",
  "repository": "https://github.com/robo-robert/gilded-rose-task.git",
  "author": "Robertas Skalskas",
  "bin": "bin/cmd",
  "scripts": {
    "refactor": "nodemon",
    "clean-test-output": "rimraf .nyc_output",
    "clean-build": "rimraf dist",
    "clean-logs": "rimraf logs/*.log",
    "clean": "yarn clean-test-output && yarn clean-build && yarn clean-logs",
    "pretest": "yarn clean",
    "test": "nyc mocha",
    "build": "yarn clean && tsc",
    "publish:local": "yarn build & npm link",
    "unpublish:local": "npm unlink"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/co": "^4.6.2",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.28",
    "@types/node-fetch": "^2.5.8",
    "@types/yargs": "^16.0.0",
    "chai": "^4.3.0",
    "diff": "^5.0.0",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "co": "^4.6.0",
    "node-fetch": "^2.6.1",
    "winston": "^3.3.3",
    "yargs": "^16.2.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text"
    ]
  }
}